array int arrayGlobal[100];

function int Find(int num, int size) {
     int i= 0;    
     for(i < size; i = i +1;) {
         if(arrayGlobal[i] == num){
            return i;
         }
     }
     return -1;
}

main() {
    int i = 99;
    int j = 0;
    for(i > -1; i = i - 1;){
        arrayGlobal[i] = j;
        j = j + 1;
    }
    console(Find(20,100));

}